{
    "cli": {
      "version": ">= 3.8.1",
      "appVersionSource": "remote"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "env": {
          "APP_VARIANT": "development"
        },
        "ios": {
          "resourceClass": "m-medium"
        },
        "channel": "development"
      },
      "preview": {
        "distribution": "internal",
        "env": {
          "APP_VARIANT": "preview"
        },
        "ios": {
          "resourceClass": "m-medium"
        },
        "android": {
          "buildType": "apk"
        },
        "channel": "preview"
      },
      "production-apk": {
        "env": {
          "APP_VARIANT": "production"
        },
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        },
        "ios": {
          "resourceClass": "m-medium"
        },
        "channel": "production"
      },
      "production": {
        "env": {
          "APP_VARIANT": "production"
        },
        "ios": {
          "resourceClass": "m-medium"
        },
        "channel": "production",
        "autoIncrement": true
      }
    },
    "submit": {
      "production": {
        "android": {
          "track": "production"
        }
      }
    }
  }
  